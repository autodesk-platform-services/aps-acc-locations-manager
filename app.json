{
    "name": "aps-acc.locations.manager",
    "description": "A sample showing how to manage ACC **Locations** data and import locations from the selected Revit model.",
    "repository": "https://github.com/autodesk-platform-services/aps-acc.locations.manager",
    "logo": "https://avatars.githubusercontent.com/u/114497564?s=200&v=4",
    "keywords": [
        "autodesk",
        "aps",
        "acc",
        "locations",
        "data-management",
        "model-properties",
        "revit"
    ],
    "env": {
        "APS_CLIENT_ID": {
            "description": "APS Client ID"
        },
        "APS_CLIENT_SECRET": {
            "description": "APS Client Secret"
        },
        "APS_CALLBACK_URL": {
            "description": "Callback URL of your APS app, required for 3-legged OAuth",
            "value": "https://<<YOUR HEROKU APP NAME HERE>>.herokuapp.com/api/aps/callback/oauth"
        }
    },
    "website": "https://developer.autodesk.com/",
    "success_url": "/",
    "buildpacks": [
        {
            "url": "https://github.com/jincod/dotnetcore-buildpack.git"
        }
    ]
}
